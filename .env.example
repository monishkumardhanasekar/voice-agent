# Copy this file to .env and fill in the values. Do not commit .env.

# Vapi API key (from dashboard.vapi.ai)
VAPI_API_KEY=

# Vapi phone number ID (the number we call FROM; from Vapi dashboard)
VAPI_PHONE_NUMBER_ID=

# Destination number for outbound calls 
VAPI_DESTINATION_NUMBER=

# Webhook base URL (e.g. https://your-ngrok-url.ngrok.io) â€” must be publicly reachable
# This is what you configure as the Vapi Server URL, for example:
#   https://<your-ngrok-subdomain>.ngrok.io/webhook/vapi
WEBHOOK_BASE_URL=

# OpenAI API key (for evaluation LLM)
OPENAI_API_KEY=

# Optional: webhook server host/port for local development
# WEBHOOK_HOST=0.0.0.0
# WEBHOOK_PORT=8765

# Optional: patient timezone for current date/time in prompts (IANA name, default America/Chicago)
# PATIENT_TIMEZONE=America/Chicago
